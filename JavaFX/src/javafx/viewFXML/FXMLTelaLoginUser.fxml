            <Cursor fx:constant="HAND" />
         </cursor></TextField>
      <Hyperlink fx:id="link" alignment="CENTER" layoutX="111.0" layoutY="244.0" onAction="#actionAbrirTelaCadastro" text="Cadastrar" textAlignment="CENTER" textFill="WHITE" underline="true">
         <font>
            <Font name="Arial Black" size="12.0" />
