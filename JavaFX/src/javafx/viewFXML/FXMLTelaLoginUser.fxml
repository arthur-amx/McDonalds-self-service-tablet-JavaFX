<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="349.0" prefWidth="470.0" styleClass="body" stylesheets="@../styles/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.ControlerFXML.FXMLTelaLoginUserController">
   <children>
      <PasswordField fx:id="usuarioSenha" layoutX="111.0" layoutY="212.0" prefHeight="25.0" prefWidth="213.0">
